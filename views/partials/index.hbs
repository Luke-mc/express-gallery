
<div id="left_column">

  <div id="first">
    {{#each data}}
      {{#if @last}}
        {{author}} <br><br>
        <img src="{{link}}"><br><br><br>
      {{/if}}
    {{/each}}
  </div>
</div>

<div class="pic_container">

    {{#each data}}
      {{#unless @last}}
        <div class= "pictures"><br>
          <img src="{{link}}">
         {{author}} <br>
        </div>
        {{/unless}}
    {{/each}}
</div>
